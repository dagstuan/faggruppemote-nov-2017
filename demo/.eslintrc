{
  'extends': 'airbnb',
  'parser': 'babel-eslint',
  'rules': {
    'react/jsx-filename-extension': [1, { 'extensions': ['.js', '.jsx'] }],
    'import/prefer-default-export': [0],
    'react/prop-types': [0],
    'no-console': [0],
    'arrow-body-style': [0],
    'no-multiple-empty-lines': [0],
  },
  'env': {
    'browser': true,
  },
}
